generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id          Int      @id @default(autoincrement())
  description String
  category    String
  cost        Int
  currency    String
  images      String[]
  name        String
  stockCount  Int
}
